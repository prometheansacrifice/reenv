{
  "name": "reenv",
  "version": "0.3.0",
  "description": "dotenv-cli written in reason",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "bin": [
        "reenv"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "rely/lib",
        "reenv/lib"
      ],
      "bin": "TestReenv.re",
      "ocamloptFlags": ["-linkall", "-g"],
      "rawBuildConfig": [
        "(deps fixtures/.env fixtures/.env.empty fixtures/.env.json fixtures/.env.new_line fixtures/.env.quotes fixtures/.env.single_quotes fixtures/.env.trim fixtures/.env.with_comment fixtures/.env.with_empty_line)"
      ]
    },
    "lib": {
      "namespace": "Reenv",
      "require": [
        "unix",
        "re"
      ]
    },
    "executable": {
      "require": [
        "reenv/lib",
        "cmdliner"
      ],
      "bin": {
        "reenv": "ReenvApp.re"
      },
      "rawBuildConfig": [
        "(package reenv)"
      ],
      "rawBuildConfigFooter": [
        "(env (_ (flags (:standard -w -39))))"
      ]
    }
  },
  "scripts": {
    "test": "esy b dune runtest --force",
    "coverage": "esy bisect-ppx-report -html _coverage #{self.target_dir}/default/test/bisect0001.out -verbose ",
    "doc": "esy dune build @doc",
    "format": "esy dune build @fmt --auto-promote"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": ">= 3.4.0 < 3.5.0",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "@reason-native/pastel": "*",
    "@reason-native/rely": "*",
    "@opam/cmdliner": "*",
    "@opam/ppxlib": "*",
    "@opam/re": "*"
  },
  "resolutions": {
    "@opam/ppxlib": "ocaml-ppx/ppxlib:ppxlib.opam#3bdccbb09b94acdf1f29e3cf57a263aea82e72fa",
    "@opam/ocaml-migrate-parsetree": "ocaml-ppx/ocaml-migrate-parsetree:ocaml-migrate-parsetree.opam#a5be5e1b94f02c667ab354c2e6b55baedeb30187",
    "@opam/base": "v0.11.1",
    "pesy": "esy/pesy#fde323697a0357003d16fb6e057d7c7b0ee293fd"
  },
  "devDependencies": {
    "@opam/bisect_ppx": "*",
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@opam/odoc": "1.4.0"
  }
}
